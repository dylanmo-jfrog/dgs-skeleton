{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "eks:*",
                "ec2:*",
                "iam:*",
                "elasticloadbalancing:*",
                "autoscaling:*",
                "kms:*",
                "logs:*"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:*"
            ],
            "Resource": [
                "arn:aws:s3:::dgs-skeleton-terraform-state",
                "arn:aws:s3:::dgs-skeleton-terraform-state/*"
            ]
        }
    ]
}
