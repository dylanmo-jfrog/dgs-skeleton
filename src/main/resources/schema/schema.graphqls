type Query {
  shows(titleFilter: String): [Show]
  diagnosis: TechDetail
  kubeinfo: KubeAmbassador
}

type Mutation {
  addShow(input: ShowInput): Show
}

input ShowInput {
  title: String
  releaseYear: Int
}
type Show {
  title: String
  releaseYear: Int
}

type KubeAmbassador {
  token: String
  namespace: String
  cert: String
}

type TechDetail {
  hostname: String
  authEndpoint: String
  podIp: String
  podName: String
}
